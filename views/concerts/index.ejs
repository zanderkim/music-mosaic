<%- include('../partials/header') %>
<container id="main-concert-page">
<table id="concert-list">
    <thead id="concert-headings">
        <tr>
            <th> your mosaic </th>
        </tr>
    </thead>
    <tbody>
        <% concerts.forEach(function(c) { %>
           
            <tr>
                <td id="bold-artist">test <%= c.artist %> </td>
                <td> <%= c.tour %></td>
                <td> <%= c.venue %></td>
                <td> <%= c.location %></td>
                <td> <%= c.date %></td>
                <td> <%= c.concertType %></td>
                <td> <a href="/concerts/<%= c._id %>">details</a></td>
            </tr>
        
            <% }) %>
    </tbody>
</table>
</container>

<%- include('../partials/footer') %>
